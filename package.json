{
  "name": "@react-hive/honey-style",
  "version": "0.0.5",
  "description": "",
  "keywords": [
    "react",
    "styled-components",
    "typescript"
  ],
  "homepage": "https://github.com/React-Hive/honey-style",
  "bugs": {
    "url": "https://github.com/React-Hive/honey-style/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/React-Hive/honey-style.git"
  },
  "author": "Mykhailo Aliinyk <m.aliynik@gmail.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && NODE_ENV=production webpack --config webpack.config.js",
    "test": "jest --collect-coverage",
    "diagnostics": "node --max-old-space-size=4096 ./node_modules/typescript/bin/tsc --project ./tsconfig.build.json --extendedDiagnostics --generateTrace ts-trace",
    "analyze": "analyze-trace ts-trace"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "!dist/**/__mocks__",
    "!dist/**/__tests__",
    "!dist/**/jest*"
  ],
  "peerDependencies": {
    "react": "^19.0.0"
  },
  "dependencies": {
    "csstype": "3.1.3",
    "stylis": "4.3.6",
    "@mdx-js/react": "3.1.0"
  },
  "devDependencies": {
    "@eslint/js": "9.24.0",
    "@mdx-js/loader": "3.1.0",
    "@testing-library/react": "16.3.0",
    "@types/jest": "29.5.14",
    "@types/mdx": "2.0.13",
    "@types/react": "^19.1.0",
    "@types/stylis": "4.2.7",
    "eslint": "9.24.0",
    "eslint-plugin-react": "7.37.5",
    "html-webpack-plugin": "5.6.3",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "prettier": "3.5.3",
    "ts-jest": "29.3.1",
    "ts-loader": "9.5.2",
    "typescript": "5.8.3",
    "typescript-eslint": "8.29.1",
    "webpack": "5.99.0",
    "webpack-cli": "6.0.1"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "testMatch": [
      "**/src/**/__tests__/**/*.tsx"
    ]
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ]
}