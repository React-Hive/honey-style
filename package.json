{
  "name": "@react-hive/honey-style",
  "version": "2.0.0",
  "description": "",
  "keywords": [
    "react",
    "styled-components",
    "typescript"
  ],
  "homepage": "https://github.com/React-Hive/honey-style",
  "bugs": {
    "url": "https://github.com/React-Hive/honey-style/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/React-Hive/honey-style.git"
  },
  "author": "Mykhailo Aliinyk <m.aliynik@gmail.com>",
  "license": "MIT",
  "scripts": {
    "prepare": "husky",
    "build": "webpack --config webpack.config.mjs",
    "test": "jest --collect-coverage",
    "performanceTest": "jest --testMatch **/*.performance.spec.tsx --testPathIgnorePatterns=''",
    "diagnostics": "tsx scripts/collect-diagnostics.ts"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.cjs"
  },
  "files": [
    "dist",
    "!dist/**/__mocks__",
    "!dist/**/__tests__",
    "!dist/**/jest*"
  ],
  "peerDependencies": {
    "react": "^19.0.0",
    "styled-components": "^6.1.0"
  },
  "dependencies": {
    "@mdx-js/react": "3.1.0",
    "csstype": "3.1.3",
    "stylis": "4.3.6"
  },
  "devDependencies": {
    "@eslint/js": "9.29.0",
    "@mdx-js/loader": "3.1.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.3.0",
    "@types/jest": "29.5.14",
    "@types/mdx": "2.0.13",
    "@types/node": "22.15.31",
    "@types/react": "^19.1.8",
    "@types/stylis": "4.2.7",
    "copy-webpack-plugin": "13.0.0",
    "eslint": "9.29.0",
    "eslint-plugin-react": "7.37.5",
    "html-webpack-plugin": "5.6.3",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "prettier": "3.5.3",
    "ts-jest": "29.4.0",
    "ts-loader": "9.5.2",
    "tsx": "4.20.3",
    "typescript": "5.8.3",
    "typescript-eslint": "8.34.1",
    "webpack": "5.99.9",
    "webpack-cli": "6.0.1"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/jest-setup.ts"
    ],
    "testMatch": [
      "**/src/**/__tests__/**/*.spec.ts?(x)"
    ],
    "testPathIgnorePatterns": [
      "\\.performance\\.spec\\.tsx$"
    ]
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ]
}
